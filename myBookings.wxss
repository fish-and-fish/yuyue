/* pages/myBookings/myBookings.wxss */
.container {
  padding: 20px;
}
.tabs {
  display: flex;
  justify-content: space-around;
  margin-bottom: 20px;
}
.tab {
  padding: 10px;
  border-bottom: 2px solid transparent;
}
.tab.active {
  border-bottom: 2px solid #007aff;
}
.booking {
  padding: 20px;
  border: 1px solid #ccc;
  margin-bottom: 20px;
}
.course-name {
  font-size: 18px;
  margin-bottom: 10px;
}
.time, .teacher, .actions {
  margin-bottom: 10px;
}
.actions {
  display: flex;
  justify-content: space-between;
}
.nav-bar {
  display: flex;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  border-top: 1px solid #ddd;
}
.container {
  padding: 20px;
}
.course {
  display: flex;
  align-items: center;
  margin-bottom: 20px;
}
.course-image {
  width: 50px;
  height: 50px;
  margin-right: 10px;
}
.nav-bar {
  display: flex;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  border-top: 1px solid #ddd;
}
